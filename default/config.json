{
    "autopreview": {
        "image": true,
        "todolist": true,
        "iframe": true,
        "anchor": true,
        "math": true
    },
    "autopreview-security": true,
    "editor": {
        "font-size": "16px"
    },
    "max-recent": 5,
    "highlight-modes": "",
    "preview-template": "default",
    "startup-commands": {
        "showBlocks": false,
        "showHiddenCharacters": true,
        "showTocPane": false,
        "autoCloseBrackets": true
    },
    "spellchecker": {
        "active": true,
        "language": "en_US"
    },
    "window": {
        "showMenuBar": true
    }
}
